{
  "name": "@sixa/wp-block-utils",
  "version": "0.3.2",
  "description": "A library of useful functions and components to be used across all sixa WordPress blocks.",
  "main": "dist/index.js",
  "browser": "dist/wp-block-utils.min.js",
  "module": "dist/es/index.js",
  "unpkg": "dist/wp-block-utils.min.js",
  "style": "dist/wp-block-utils.min.css",
  "scripts": {
    "build": "npm run build:js && npm run build:css",
    "build:js": "rollup -c",
    "build:css": "postcss --config ./postcss.config.js src/**/*.css -d dist",
    "lint:css": "./node_modules/stylelint/bin/stylelint.js \"src/**/*.css\"",
    "lint:css-fix": "./node_modules/stylelint/bin/stylelint.js \"src/**/*.css\" --fix",
    "lint:js": "./node_modules/eslint/bin/eslint.js src",
    "lint:js-fix": "./node_modules/eslint/bin/eslint.js src --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sixach/sixa-wp-block-utils.git"
  },
  "author": "sixa AG",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/sixach/sixa-wp-block-utils/issues"
  },
  "keywords": [
    "sixa",
    "block",
    "sixa blocks",
    "wordpress",
    "wordpress gutenberg",
    "gutenberg block",
    "wordpress gutenberg block",
    "utilities"
  ],
  "homepage": "https://github.com/sixach/sixa-wp-block-utils#readme",
  "dependencies": {
    "classnames": "^2.2.6",
    "lodash-es": "^4.17.21",
    "rename-keys": "^2.0.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.12.13",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-replace": "^2.4.1",
    "@wordpress/eslint-plugin": "^9.0.1",
    "@wordpress/stylelint-config": "^19.0.1",
    "eslint": "^7.22.0",
    "husky": "^4.3.8",
    "postcss": "^7.0.35",
    "postcss-cli": "^7.1.2",
    "postcss-custom-media": "^7.0.8",
    "postcss-discard-empty": "^4.0.1",
    "postcss-each": "^0.10.0",
    "postcss-if-media": "^1.0.3",
    "rollup": "^2.42.3",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-local-resolve": "^1.0.7",
