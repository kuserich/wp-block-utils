{
	"name": "@sixach/wp-block-utils",
	"version": "1.0.1",
	"description": "A collection of JavaScript utility methods delivering modularity, performance & extras.",
	"keywords": [
		"gutenberg",
		"wordpress",
		"utils",
		"react"
	],
	"homepage": "https://github.com/sixach/wp-block-utils#readme",
	"bugs": {
		"url": "https://github.com/sixach/wp-block-utils/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sixach/wp-block-utils.git"
	},
	"license": "GPL-3.0-or-later",
	"author": "sixa AG",
	"sideEffects": false,
	"main": "build/index.js",
	"module": "build/index.js",
	"scripts": {
		"build": "cross-env BABEL_ENV=default NODE_ENV=production && rm -rf build && mkdir build && babel src --out-dir build --copy-files",
		"check-engines": "wp-scripts check-engines",
		"check-licenses": "wp-scripts check-licenses",
		"format": "wp-scripts format",
		"lint:js": "wp-scripts lint-js",
		"lint:js-fix": "wp-scripts lint-js --fix",
		"lint:pkg-json": "wp-scripts lint-pkg-json",
		"packages-update": "wp-scripts packages-update",
		"prepublishOnly": "npm run format && npm run build && npm run docs",
		"test:e2e": "wp-scripts test-e2e",
		"test:unit": "wp-scripts test-unit-js",
		"test:unit:help": "wp-scripts test-unit-js --help",
		"test:unit:watch": "wp-scripts test-unit-js --watch",
		"test:unit:debug": "wp-scripts --inspect-brk test-unit-js --runInBand --no-cache",
		"docs": "documentation build src/** -f html --github -o docs"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,ts,tsx}": [
			"npm run lint:js",
			"npm run format:js"
		]
	},
	"babel": {
		"presets": [
			"@babel/preset-env",
			"@babel/preset-react"
		]
	},
	"dependencies": {
		"rename-keys": "^2.0.1",
		"slugify": "^1.6.0",
		"striptags": "^3.2.0"
	},
	"devDependencies": {
		"@babel/cli": "7.14.5",
		"@babel/core": "7.14.6",
		"@babel/preset-env": "7.14.7",
		"@babel/preset-react": "7.14.5",
		"@wordpress/scripts": "16.1.4",
		"classnames": "2.3.1",
		"cross-env": "7.0.3",
		"husky": "^7.0.1",
		"lint-staged": "11.0.1",
		"lodash": "4.17.21"
	},
	"peerDependencies": {
		"@wordpress/api-fetch": "5.x",
		"@wordpress/html-entities": "3.x",
		"@wordpress/i18n": "4.x",
		"@wordpress/url": "3.x"
	},
	"engines": {
		"node": ">=16",
		"npm": ">=7.0"
	},
	"publishConfig": {
		"registry": "https://npm.pkg.github.com/"
	}
}
